{
  "name": "koa2-api",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "6.9.0",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "export NODE_CONFIG_DIR='./app/config' && ./node_modules/.bin/nodemon app/server.js --exec babel-node",
    "build": "./node_modules/.bin/babel app -d dist",
    "serve": "export NODE_CONFIG_DIR='./dist/config' && node dist/index.js",
    "test": "export SUPPRESS_NO_CONFIG_WARNING=true && export NODE_CONFIG_DIR='./app/config' && ./node_modules/.bin/mocha --timeout 3000 --require ./test/mocha-babel",
    "lint": "./node_modules/.bin/eslint .",
    "docs": "echo Docs!",
    "validate": "npm ls",
    "check": "./node_modules/.bin/ncu",
    "update": "./node_modules/.bin/ncu -u",
    "postinstall": "npm run build && npm run docs",
    "precommit": "npm run lint",
    "prepush": "npm test"
  },
  "keywords": [
    "koa2",
    "api"
  ],
  "author": "Andrei Lavrenyuk",
  "license": "ISC",
  "dependencies": {
    "app-utils": "git+https://github.com/bagbee/nodejs-app-utils#master",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-preset-es2016": "6.22.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "chai-json-schema": "1.4.0",
    "chai-like": "0.1.10",
    "chai-things": "0.2.0",
    "config": "1.21.0",
    "crypto-js": "3.1.8",
    "debug": "2.2.0",
    "koa": "2.0.0-alpha.7",
    "koa-bodyparser": "2.2.0",
    "koa-context-validator": "0.2.0",
    "koa-convert": "1.2.0",
    "koa-error": "2.1.0",
    "koa-errorhandler": "0.1.1",
    "koa-json": "2.0.2",
    "koa-router": "7.0.1",
    "mocha": "3.1.2",
    "moment": "2.15.2",
    "mongoose": "4.6.5",
    "mongoose-beautiful-unique-validation": "5.1.1",
    "mw-authentication": "git+https://github.com/bagbee/mw-authentication.git#master",
    "mw-pagination": "git+https://github.com/bagbee/mw-pagination.git#master",
    "pm2": "2.0.19",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-preset-es2016": "6.22.0",
    "eslint": "3.9.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.0.1",
    "husky": "0.11.9",
    "nodemon": "1.11.0",
    "npm-check-updates": "2.8.6"
  }
}
